{
  "imports": {
    "std/": "https://deno.land/std@0.208.0/",
    "hono/": "https://deno.land/x/hono@v3.11.11/",
    "hono/jsx/jsx-runtime": "https://deno.land/x/hono@v3.11.11/jsx/jsx-runtime.ts"
  },
  "lock": false,
  "tasks": {
    "cms": "deno run --unstable --watch -A probas/ensino.ts",
    "site": "cd probas/ensino && deno task serve",
    "dev": "deno task cms & deno task site"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx"
  },
  "fmt": {
    "exclude": [
      "probas"
    ]
  }
}
